        <argument>
          <name>RecordMedium</name>
          <relatedStateVariable>RecordStorageMedium</relatedStateVariable>
          <direction>out</direction>
        </argument>
        <argument>
          <name>WriteStatus</name>
          <relatedStateVariable>RecordMediumWriteStatus</relatedStateVariable>
          <direction>out</direction>
        </argument>
      </argumentList>
